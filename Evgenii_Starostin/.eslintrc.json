{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-debugger": 0,
    "react/no-array-index-key": 0,
    "import/no-unresolved": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/jsx-one-expression-per-line": 0,
    "no-underscore-dangle": 0,
    "no-confusing-arrow": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}
